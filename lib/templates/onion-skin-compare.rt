<div class="onion-skin-compare-contents">
	<div class="onion-skin-compare-top-wrapper" style="opacity: {this.state.percent.toFixed(2)}">
		<img class="onion-skin-compare-top-image" src="{this.props.image_1}" />
	</div>
	<div class="onion-skin-compare-bottom-wrapper">
		<img class="onion-skin-compare-bottom-image" src="{this.props.image_2}" />
	</div>
	<div class="slider-wrapper"></div>
	<div class="onion-skin-info">{this.state.percent_string}</div>
</div>